    // Generate a json string for successful parsing unittests
    public static func getPositiveStr() -> String {
        var json = "{";
{{ let mut r_idx = 0;
let mut r_max_idx = typ.attributes.len(); 
for attr in typ.attributes.iter() { }}
        json += "\" {{= attr.name }}\"";
        json += ":";
{{ if attr.is_array { }}
        json += "[";
{{ } if model::Type::is_basic_type(&attr.attribute_type) { }}
        json += " {{= get_test_value(&attr.attribute_type) }}";
{{ } else { }}
        json += Test {{= attr.attribute_type }}.getPositiveStr();
{{ } if attr.is_array { }}
        json += "]";
{{ } }}
{{   r_idx += 1;
  if r_idx < r_max_idx { }}
        json += ",";
{{   }
} }}
        json += "}";
        return json;
    }